{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/FullCheckLazy",
    "definitions": {
        "FullCheckLazy": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "msg_id": {
                    "type": "string",
                    "format": "uuid"
                },
                "msisdn": {
                    "type": "string"
                },
                "first_name": {
                    "type": "string"
                },
                "surname": {
                    "type": "string"
                },
                "patronymic": {
                    "type": "string"
                },
                "birth_date": {
                    "type": "string",
                    "format": "date"
                },
                "series": {
                    "type": "string",
                    "format": "integer"
                },
                "number": {
                    "type": "string",
                    "format": "integer"
                },
                "document_type": {
                    "type": "integer"
                },
                "issue_date": {
                    "type": "string",
                    "format": "date"
                },
                "issue_authority": {
                    "type": "string"
                },
                "callback_url": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "smev_ttl": {
                    "type": "integer"
                },
                "smev_priority": {
                    "type": "string",
                    "enum": [
                        "low",
                        "medium",
                        "high"
                    ],
                    "default": "medium"
                },
                "smev_ignore_cache": {
                    "type": "boolean",
                    "default": false
                }
            },
            "required": [
                "birth_date",
                "document_type",
                "first_name",
                "msg_id",
                "number",
                "patronymic",
                "series",
                "surname"
            ],
            "title": "FullCheckLazy"
        }
    }
}
